{
  "expo_README": "https://docs.expo.io/versions/v32.0.0/workflow/configuration/",
  "expo": {
    "name": "Improv Tilapia",
    "slug": "improv-tilapia",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "icon": "./assets/icon-android.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3f51b5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.cooperka.improv",
      "icon": "./assets/icon-android.png",
      "permissions": []
    },
    "ios": {
      "buildNumber": "3",
      "bundleIdentifier": "com.cooperka.improv",
      "icon": "./assets/icon-ios.png",
      "supportsTablet": true
    },
    "description": "Simple tools to help your team practice smarter.",
    "githubUrl": "https://github.com/cooperka/ImprovTilapia"
  }
}
